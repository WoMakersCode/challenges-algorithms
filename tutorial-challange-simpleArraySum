public class SimpleArraySum {
    public static void main(String args[]){
        int[] arr={1,2,3,34,126,200,122};
        System.out.println(Sum(arr));

    }
    static int Sum(int[] arr){
        int n=arr.length;
        int sum=0;
        for (int i=0;i<n;i++){
            sum+=arr[i];
        }
        return sum;

    }
    
}
